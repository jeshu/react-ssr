{
  "name": "react-ssr",
  "version": "1.0.0",
  "description": "Server side rendering project",
  "main": "index.js",
  "scripts": {
    "start": "npm-run-all prod:*",
    "prod:build": "",
    "prod:build-server": "webpack",
    "prod:build-client": "",
    "dev": "npm-run-all --parallel dev:*",
    "dev:run-server": "nodemon --watch build --exec node build/bundle.js",
    "dev:build-server": "webpack --config webpack.server.js --watch",
    "dev:build-client": "webpack --config webpack.client.js --watch",
    "test": "jest"
  },
  "author": "",
  "license": "ISC",
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/index.js"
    ],
    "coverageReporters": [
      "text"
    ]
  },
  "dependencies": {
    "axios": "^0.16.2",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2017": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "compression": "^1.7.4",
    "concurrently": "^3.6.1",
    "express": "^4.17.1",
    "express-http-proxy": "^1.6.0",
    "lodash": "^4.17.15",
    "nodemon": "^1.19.4",
    "npm-run-all": "^4.1.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^5.2.1",
    "react-redux": "^5.1.2",
    "react-router": "^5.1.2",
    "react-router-dom": "^4.3.1",
    "redux": "^3.7.2",
    "redux-thunk": "^2.3.0",
    "serialize-javascript": "^1.9.1",
    "webpack": "^3.12.0",
    "webpack-dev-server": "^2.11.5",
    "webpack-merge": "^4.2.2",
    "webpack-node-externals": "^1.7.2"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.9.5",
    "babel-jest": "^25.3.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.4.4",
    "jest": "^25.3.0"
  }
}
